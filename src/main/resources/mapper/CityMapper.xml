<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ln.mapper.CityMapper">
    <select id="insertCity" resultType="CityBean">
        select * from tb_city where pid=1
    </select>

    <select id="insertCityBypid" resultType="CityBean">
        select * from tb_city where pid=#{pid}
    </select>

</mapper>